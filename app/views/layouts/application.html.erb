<!DOCTYPE html>
<html>
<head>
  <title>Gallery</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= yield(:head) %>
</head>
<body>

	<header class="navbar navbar-fixed-top">
		<div class="navbar-inner">
			<div class="container">
				<%= render 'layouts/navigation' %>
			</div>
	</header>

	<div id="main" role="main">
		<div class="container">
			<div class="content">
				<div class="row">
					<div class="span12">
						<%= yield %>
					</div>
				</div>

			<footer>
			</footer>
		</div>
	</div>

</body>
</html>
